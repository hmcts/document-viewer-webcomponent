    "otp": "^0.1.3",
    "request-promise": "^4.2.2",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.3.3",
    "scss-bundle": "^2.4.0",
    "session-file-store": "1.2.0",
    "yarn": "^1.13.0",
    "zone.js": "~0.8.26"
