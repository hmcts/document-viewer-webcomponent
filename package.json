{
  "name": "@hmcts/annotation-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:node && ng build --prod && ng run annotation-ui:serve",
    "build:node": "cd api && npm run build",
    "start-dev-proxy": "ng serve --proxy-config proxy.config.json",
    "watch-dev-node": "cd api && npm run watch",
    "postBuild": "ts-node ./scripts/css-bundle.ts",
    "copy-license": "cp ./README.md dist/hmcts-annotation-ui-lib",
    "copy-readme": "cp ./LICENSE dist/hmcts-annotation-ui-lib",
    "copy-files": "npm run copy-readme && npm run copy-license",
    "build-ci": "ng build --prod hmcts-annotation-ui-lib && cp -R projects/hmcts-annotation-ui-lib/src/assets/ dist/hmcts-annotation-ui-lib/assets/",
    "npm-pack": "cd dist/hmcts-annotation-ui-lib && npm pack",
    "package": "npm run build-ci && npm run copy-files && npm run npm-pack",
    "test": "ng test",
    "lint": "ng lint",
    "test:codacy-upload": "cat ./reports/tests/coverage/lcov.info | codacy-coverage",
    "test:codecov-upload": "codecov -f ./reports/tests/coverage/lcov.info",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-ng-packagr": "^0.12.1",
    "@angular/animations": "6.1.0",
    "@angular/cdk": "6.1.0",
    "@angular/cli": "6.1.0",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/material": "6.1.0",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/platform-server": "6.1.0",
    "@angular/router": "6.1.0",
    "@hmcts/frontend": "0.0.14-alpha",
    "@hmcts/info-provider": "1.0.0",
    "@hmcts/nodejs-healthcheck": "1.4.6",
    "@hmcts/nodejs-logging": "3.0.0",
    "@ng-toolkit/universal": "^1.1.45",
    "@nguniversal/express-engine": "6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "6.0.0",
    "applicationinsights": "1.0.8",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.6.1",
